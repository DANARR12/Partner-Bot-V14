{
  "name": "discord-partner-bot",
  "version": "1.0.0",
  "description": "A Discord bot for handling server partnerships and advertisements",
  "main": "bot.js",
  "scripts": {
    "start": "pm2 start bot.js --name discord-bot --time",
    "start:all": "pm2 start ecosystem.config.js",
    "start:xp": "pm2 start discord_xp_bot.py --name discord-xp-bot --interpreter python3 --time",
    "stop": "pm2 stop discord-bot",
    "stop:all": "pm2 stop all",
    "stop:xp": "pm2 stop discord-xp-bot",
    "restart": "pm2 restart discord-bot",
    "restart:all": "pm2 restart all",
    "restart:xp": "pm2 restart discord-xp-bot",
    "status": "pm2 status",
    "logs": "pm2 logs discord-bot",
    "logs:xp": "pm2 logs discord-xp-bot",
    "logs:all": "pm2 logs",
    "dev": "nodemon bot.js"
  },
  "keywords": [
    "discord",
    "bot",
    "partnership",
    "advertisement"
  ],
  "author": "SmSm",
  "license": "ISC",
  "dependencies": {
    "@discordjs/util": "^1.1.1",
    "@discordjs/voice": "^0.16.1",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "ms": "^2.1.3",
    "multiple.db": "^0.0.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
